<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Portfoglio by Ilya Gorshkov</title>
    <script src="//cdn.ckeditor.com/4.10.0/standard/ckeditor.js"></script>
    @* <environment include="Production"> *@
        <link rel="stylesheet" href="~/css/site.css"/>
        <link rel="stylesheet" href="~/css/uikit.css"/>
    @* </environment> *@
    @* <environment exclude="Production"> *@
        <link rel="stylesheet" href="~/css/uikit.min.css" asp-append-version="true"/>
    @* </environment> *@
</head>
<body>
<nav class="uk-navbar-container uk-margin" uk-navbar uk-sticky>
    <div class="uk-navbar-center">

        <div class="uk-navbar-center-left">
            <div>
                <ul class="uk-navbar-nav">
                    <li class="uk-active">
                        <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li>
                        <a href="#">More</a>
                        <div class="uk-navbar-dropdown">
                            <ul class="uk-nav uk-navbar-dropdown-nav">
                                <li>
                                    <a asp-area="" asp-controller="Art" asp-action="Index">Art</a>
                                </li>
                                <li>
                                    <a href="https://tasker.ilya-gorshkov.ru/">Tasker</a>
                                </li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
        <a class="uk-navbar-item uk-logo" asp-area="" asp-controller="Home" asp-action="Index">Ilya Gorshkov</a>
        <div class="uk-navbar-center-right">
            <div>
                <ul class="uk-navbar-nav">
                    <li>
                        <a asp-area="" asp-controller="Home" asp-action="About">About me</a>
                    </li>
                </ul>
            </div>
        </div>

    </div>
</nav>
<div>
                @RenderBody()
            
    <footer id="footer" class="uk-section uk-section-xsmall uk-background-muted">
        <div class="uk-container uk-container-small">&copy; 2018 - Portfoglio by Ilya Gorshkov @ViewData["Footer"]</div>
    </footer>
</div>

