@model IEnumerable<IAlbum>

@{
    Layout = "_ArtLayout";
    ViewData["Title"] = "Art";
    ViewData["Footer"] = "| Pictures by Ksenia Kallagova";
}
        <div class="uk-text-center" uk-grid>
            @if (Model.Any())
            {
                foreach (var album in Model)
                {
                    @await Html.PartialAsync("ShowAlbum", album)
                }
            }
        </div>